{
  "manifest_version": 3,
  "name": "Chrome Extension Project",
  "version": "1.0",
  "description": "A sample Chrome extension project.",
  "permissions": [
    "activeTab",
    "tabs",
    "sidepanel"
  ],
  "background": {
    "service_worker": "background.js",
    "side_panel": "sidepanel.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}